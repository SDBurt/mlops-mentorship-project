# Example: Dagster User Code Secrets
# Copy this file to user-code-secrets.yaml and replace placeholders with actual values
# DO NOT commit user-code-secrets.yaml - it is gitignored

apiVersion: v1
kind: Secret
metadata:
  name: dagster-user-code-secrets
  namespace: lakehouse
type: Opaque
stringData:
  # Polaris REST Catalog Credentials
  # Obtain these by running: infrastructure/kubernetes/polaris/init-polaris.sh
  # Credentials saved to: infrastructure/kubernetes/polaris/.credentials/dagster_user.txt
  POLARIS_CLIENT_ID: "YOUR_POLARIS_CLIENT_ID_HERE"
  POLARIS_CLIENT_SECRET: "YOUR_POLARIS_CLIENT_SECRET_HERE"

  # Reddit API Credentials
  # Get credentials from: https://www.reddit.com/prefs/apps
  # Create a "script" type application for personal use
  REDDIT_CLIENT_ID: "YOUR_REDDIT_CLIENT_ID_HERE"
  REDDIT_CLIENT_SECRET: "YOUR_REDDIT_CLIENT_SECRET_HERE"
  REDDIT_USER_AGENT: "your-app-name:v1.0.0 (by /u/your_reddit_username)"

  # MinIO S3 Credentials (for PyIceberg and AWS SDK)
  # These should match your MinIO deployment credentials
  PYICEBERG_CATALOG__DEFAULT__S3__ACCESS_KEY_ID: "admin"
  PYICEBERG_CATALOG__DEFAULT__S3__SECRET_ACCESS_KEY: "YOUR_MINIO_PASSWORD_HERE"
  AWS_ACCESS_KEY_ID: "admin"
  AWS_SECRET_ACCESS_KEY: "YOUR_MINIO_PASSWORD_HERE"
