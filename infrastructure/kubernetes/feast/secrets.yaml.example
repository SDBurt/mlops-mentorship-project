# Feast Secrets
# Copy to secrets.yaml and apply before deploying Feast
#
# kubectl apply -f secrets.yaml -n lakehouse

---
apiVersion: v1
kind: Secret
metadata:
  name: feast-db-secret
  namespace: lakehouse
type: Opaque
stringData:
  # Password for 'feast' PostgreSQL user
  # Generate with: openssl rand -base64 24
  password: "<GENERATE: openssl rand -base64 24>"

---
apiVersion: v1
kind: Secret
metadata:
  name: feast-redis-secret
  namespace: lakehouse
type: Opaque
stringData:
  # Redis password
  # Generate with: openssl rand -base64 24
  password: "<GENERATE: openssl rand -base64 24>"
