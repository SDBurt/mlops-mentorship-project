{
  "event_id": "{{key "evt_" 1000000}}",
  "event_type": "charge.{{randoms "succeeded|failed"}}",
  "provider": "{{randoms "stripe|braintree|adyen|checkout|square"}}",
  "charge_id": "{{key "ch_" 500000}}",
  "customer_id": "{{key "cus_" 50000}}",
  "amount": {{amount 1 5000 ""}},
  "currency": "{{randoms "USD|EUR|GBP|CAD|JPY"}}",
  "status": "{{randoms "succeeded|failed|pending"}}",
  "failure_code": "{{randoms "card_declined|insufficient_funds|expired_card|processing_error"}}",
  "failure_message": "{{randoms "Your card was declined|Insufficient funds|Card has expired|Processing error"}}",
  "payment_method_id": "{{key "pm_" 100000}}",
  "payment_method_type": "{{randoms "card|bank_account|digital_wallet"}}",
  "card_brand": "{{randoms "visa|mastercard|amex|discover"}}",
  "card_last4": "{{integer 1000 9999}}",
  "card_exp_month": {{integer 1 12}},
  "card_exp_year": {{integer 2025 2030}},
  "card_country": "{{randoms "US|CA|GB|FR|DE|AU"}}",
  "billing_country": "{{randoms "US|CA|GB|FR|DE|AU"}}",
  "billing_postal_code": "{{randoms "12345|90210|M5H2N2|SW1A1AA|75001"}}",
  "merchant_id": "{{key "merch_" 1000}}",
  "merchant_name": "{{randoms "Acme Corp|TechStart Inc|Global Services"}}",
  "description": "{{randoms "Monthly subscription|One-time payment|Invoice payment"}}",
  "metadata": {
    "order_id": "{{key "ord_" 200000}}",
    "user_email": "{{email}}",
    "subscription_id": "{{key "sub_" 10000}}"
  },
  "risk_score": {{integer 0 100}},
  "risk_level": "{{randoms "low|medium|high"}}",
  "3ds_authenticated": "{{randoms "true|false"}}",
  "created_at": "{{format_timestamp now "2006-01-02T15:04:05.000"}}",
  "updated_at": "{{format_timestamp now "2006-01-02T15:04:05.000"}}"
}
